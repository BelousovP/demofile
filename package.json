    "long": "^3.0.3",
    "protobufjs": "^4.1.3",
    "varint": "^4.0.0"
  },
  "devDependencies": {
    "ansi-styles": "^3.2.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3"
  }
